  		&quot;BRANCH_OFFICE_K&quot;,
  		&quot;SALES_TERRITORY_K&quot;,
  		&quot;SALES_BRANCH_K&quot;,
  		&quot;CUSTOMER_GROUP_K&quot;,
  		&quot;COST_CENTER_K&quot;,
  		&quot;CONTROLLING_AREA_K&quot;,
  		&quot;CREDIT_CONTROL_AREA_K&quot;,
  		&quot;PROFIT_CENTER_K&quot;,
  		&quot;ACCOUNT_REP_K&quot;,
