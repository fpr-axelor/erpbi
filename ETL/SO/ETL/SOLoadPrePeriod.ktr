        <jsScript_script>var rowTime = new Object();
var newRow;

function incTime(time)
{
	if (time.period == 12)
	{
		time.period = 1;
		time.year++;
	}
	else time.period++;
}

&#47;&#47;
&#47;&#47;	Write the cache.  If any documents have a 0 balance, remove them.  Write the 
&#47;&#47;	cache for each period until toTime.
&#47;&#47;
function writeCache(fromTime, toTime)
{
	for (var i=0; i &lt; cache.length; i++) 
	{
		putRow(cache[i]);
	}

	incTime(fromTime);

	for (var i=0; i &lt; cache.length; i++) 
	{
		cache[i][1] = fromTime.year;
		cache[i][2] = fromTime.period;	
	}

	
	if ((fromTime.year == toTime.year) &amp;&amp; (fromTime.period == toTime.period))
	{
		return;
	}

	for (var i=0; i &lt; cache.length; i++) 
	{
		if (cache[i][5] == cache[i][6])
		{
			cache.splice(i, 1);
		}
		break;
	}

	writeCache(fromTime, toTime);

}
&#47;&#47;
&#47;&#47;	If this is the first row, set the current time, end time and save fiscal pattern
&#47;&#47;
if(getProcessCount(&quot;r&quot;) == 1) 
{
	var currTime = new Object();
	currTime.period = month(str2date(getEnvironmentVar(&quot;erpi.startdate&quot;), &quot;yyyy&#47;MM&#47;dd&quot;)) + 1;
	currTime.year = year(str2date(getEnvironmentVar(&quot;erpi.startdate&quot;), &quot;yyyy&#47;MM&#47;dd&quot;)) + 1;
	var endTime = new Object();
	endTime.period = month(str2date(getEnvironmentVar(&quot;erpi.enddate&quot;), &quot;yyyy&#47;MM&#47;dd&quot;)) + 1;
	endTime.year = year(str2date(getEnvironmentVar(&quot;erpi.enddate&quot;), &quot;yyyy&#47;MM&#47;dd&quot;)) + 1;
	var cache = new Array();
	var fiscalCode = row[0];
}

&#47;&#47;
&#47;&#47;	Retrieve the time from the row.
&#47;&#47;
rowTime.year = row[1];
rowTime.period = row[2];

&#47;&#47;
&#47;&#47;	Check rows are sorted and in range of erpi.endate
&#47;&#47;
if (rowTime.year &lt; currTime.year || rowTime.period &lt; currTime.period ||
	 rowTime.year &gt; endTime.year || rowTime.period &gt; endTime.period)
{
	trans_Status = ERROR_TRANSFORMATION;
	writeToLog(&quot;row not sorted by ascending year &amp; period or outside range of erpi.endate&quot;);
}

&#47;&#47;
&#47;&#47;	If the period changed, write the cache up to the new time.
&#47;&#47; 
if ((currTime.year != rowTime.year) || (currTime.period != rowTime.period))
	writeCache(currTime, rowTime);

&#47;&#47;
&#47;&#47;	If the document does not exist in the cache, insert it.  If it does, update the balance.
&#47;&#47;
for (var i=0; i &lt;= cache.length; i++) 
{
		if (i == cache.length)
			{
    		newRow = createRowCopy(getInputRowMeta().size());
			cache.push(newRow);
			break;
			}		
		else if (cache[i][3] == row[3])
			{
			cache[i][5] = str2num(cache[i][5]) + str2num(row[5]);
			cache[i][6] = str2num(cache[i][6]) + str2num(row[6]);
			if (cache[i][8] != 0)
				cache[i][9] = cache[i][8] - cache[i][7]
			break;
			}
}
&#47;&#47;
&#47;&#47;	If this is the last row, or the fiscal code changes write the cache through erpi.endate.  
&#47;&#47;  If the fiscal code changes start over again at the begining of time with a new cache
&#47;&#47;
if ((row[4] == true) || (row[0] != fiscalCode))
{
	writeCache(currTime, endTime);
	currTime.period = month(str2date(getEnvironmentVar(&quot;erpi.startdate&quot;), &quot;yyyy&#47;MM&#47;dd&quot;)) + 1;
	currTime.year = year(str2date(getEnvironmentVar(&quot;erpi.startdate&quot;), &quot;yyyy&#47;MM&#47;dd&quot;)) + 1;
	cache = new Array();
	fiscalCode = row[0];
}

&#47;&#47;
&#47;&#47;	Discard this row.  We write our own rows using putRow
&#47;&#47;
trans_Status = SKIP_TRANSFORMATION;


