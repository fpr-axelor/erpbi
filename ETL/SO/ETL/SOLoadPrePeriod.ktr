    <sql>SELECT	&quot;DATE_PATTERN&quot;,
		&quot;FISCAL_YEAR&quot;,
		&quot;FISCAL_PERIOD&quot;, 	
		&quot;ORDER_K&quot;, 
		&quot;AMOUNT&quot; AS &quot;ORDER_AMOUNT&quot;,
		0.00 as &quot;DELIVERY_AMOUNT&quot;,
		&quot;ORDER_DATE_K&quot;,
		&quot;DELIVERY_DATE_K&quot;,
		0 AS &quot;ORDER_TO_DELIVERY_DAYS&quot;,  
		&quot;FISCAL_PERIOD_K&quot;,
